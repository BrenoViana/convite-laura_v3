{
  "compilerOptions": {
    "lib": ["ES2022", "WebWorker"],
    "module": "esnext",
    "target": "ES2022",
    "moduleResolution": "bundler",
    "strict": true,
    "skipLibCheck": true,
    "types": ["@cloudflare/workers-types"],
    "resolveJsonModule": true,
    "isolatedModules": true
  },
  "include": ["./**/*.ts"]
}
