{
    "name":  "convite-laura-v3",
    "version":  "0.0.0",
    "scripts":  {
                    "start":  "ng serve -o",
                    "build":  "ng build",
                    "build:cf":  "ng build --base-href=/",
                    "cf:post":  "TARGET=dist/convite-laura_v3; if [ -d dist/convite-laura_v3/browser ]; then TARGET=dist/convite-laura_v3/browser; fi; cp src/_redirects \"$TARGET/_redirects\" 2\u003e/dev/null || true; cp src/_headers \"$TARGET/_headers\" 2\u003e/dev/null || true; echo \"Using output: $TARGET\"; ls -lah \"$TARGET\" || true",
                    "build:gh":  "ng build --base-href=/convite-laura_v3/",
                    "gh:post":  "cp dist/convite-laura_v3/index.html dist/convite-laura_v3/browser/404.html"
                },
    "prettier":  {
                     "printWidth":  100,
                     "singleQuote":  true,
                     "overrides":  [
                                       {
                                           "files":  "*.html",
                                           "options":  {
                                                           "parser":  "angular"
                                                       }
                                       }
                                   ]
                 },
    "private":  true,
    "dependencies":  {
